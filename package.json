{
  "name": "eviate",
  "version": "0.1.0",
  "description": "Next-gen web framework for bun.js and node.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write ./**/*.{ts,json,md}",
    "test": "vitest --run",
    "prepare": "husky install",
    "prepublishOnly": "bun run build"
  },
  "dependencies": {
    "eviate-plugin": "^0.1.0-alpha.11",
    "picocolors": "^1.0.0",
    "sweet-event-emitter": "^0.1.0",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "bun-types": "^0.1.11",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "keywords": [],
  "author": "EviateJS team",
  "license": "MIT",
  "homepage": "https://github.com/eviatejs/eviate",
  "bugs": {
    "url": "https://github.com/eviatejs/eviate/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eviatejs/eviate.git"
  }
}
